/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [
      "@angular/localize"
    ],
    "baseUrl": "./"
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
  "paths": {
    "@environments/*": ["./src/environments/*"],
    "@shared/*": "["./src/app/shared/*"],
    "@main/*": "["./src/app/main/*"]
  }
}
