{
	"name": "Account Service",
	"version": "1.0.0",
	"apiVersion": "1.0.0",
	"description": "This service is in charge of processing user signups",
	"channels":
	[
		{
			"name":"user/signedup",
			"publish":
			{
				"type":"OBJECT",
				"properties":
				{
					"displayName":
					{
						"type":"BASIC",
						"format":"STRING"
					},
					"email":
					{
						"type":"BASIC",
						"format":"STRING"
					}
				}
			}
		}
	]
}